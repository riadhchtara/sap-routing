<mvc:View
	controllerName="sap.ui.demo.wt.controller.App"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page title="{i18n>homePageTitle}">
		<headerContent>
			<Button
				icon="sap-icon://hello-world"
				press="onOpenDialog"/>
		</headerContent>
		<content>
			<Button
				text="{i18n>showHelloButtonText}"
				press="onShowHello"
				class="myCustomButton"/>
		</content>
	</Page>
</mvc:View>